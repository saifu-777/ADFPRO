{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-saif9457"
		},
		"FirstLinkedservice_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'FirstLinkedservice'"
		},
		"FirstLinkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saifstorageaccount9457.dfs.core.windows.net/"
		},
		"GitLinnkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"manTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/03c0fdea-2daf-46e5-9ba8-45d3b53a3cc9/resourceGroups/FirstResource/providers/Microsoft.Storage/storageAccounts/saifstorageaccount9457"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-saif9457_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"FirstLinkedservice_accountKey": {
						"value": "[parameters('FirstLinkedservice_accountKey')]"
					},
					"FirstLinkedservice_properties_typeProperties_url": {
						"value": "[parameters('FirstLinkedservice_properties_typeProperties_url')]"
					},
					"GitLinnkedService_properties_typeProperties_url": {
						"value": "[parameters('GitLinnkedService_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-saif9457_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"manTrigger_properties_typeProperties_scope": {
						"value": "[parameters('manTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-saif9457_ArmTemplate_0')]"
			]
		}
	]
}